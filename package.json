{
  "name": "react-complete-example",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "start:prod": "npx serve -s build",
    "test": "react-scripts test --watchAll --coverage",
    "eject": "react-scripts eject",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "mock": "export $(grep -v '^#' .env | xargs) && java -jar $WIREMOCK_PATH --root-dir=$PWD/mocks --verbose",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "e2e": "npm run cy:run",
    "e2e:watch": "yarn build && concurrently \"npm:start:prod\" \"npm:cy:open\"",
    "sonar": "export $(grep -v '^#' .env | xargs) && $SONAR_SCANNER_PATH/sonar-scanner -Dsonar.projectKey=react-scaffold -Dsonar.sources=. -Dsonar.host.url=http://localhost:9000 -Dsonar.login=$SONAR_TOKEN"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.4.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.11",
    "@types/react-router-dom": "^5.1.6",
    "axios": "^0.21.0",
    "deserialize-json-api": "^1.4.0",
    "formik": "^2.2.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.0-next.98",
    "redux": "^4.0.5",
    "typescript": "^4.1.2",
    "web-vitals": "^0.2.4"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^5.4.11",
    "@testing-library/dom": "^7.21.4",
    "@types/webpack-env": "^1.15.3",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "concurrently": "^5.3.0",
    "cypress": "^5.6.0",
    "eslint": "^7.14.0",
    "eslint-plugin-react": "^7.21.5",
    "ts-loader": "^8.0.11",
    "ts-node": "^9.0.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.ts",
      "src/**/*.tsx",
      "!src/**/*.d.ts",
      "!src/index.tsx",
      "!src/reportWebVitals.ts",
      "!src/store.ts"
    ]
  }
}
